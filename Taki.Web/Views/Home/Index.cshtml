@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>
@section scripts{
    <script>
        $(function () {
            $("h2").on("click", function () {
                $.get("/Home/GetPagingData?id=1&pageIndex=2&pageSize=20", function (result) {
                    if (result.State == "SUCCESS") {
                        alert(result.Data.StrDateTime);
                    } else {
                        alert(result.Message);
                    }
                });
            });
        });

    </script>

}